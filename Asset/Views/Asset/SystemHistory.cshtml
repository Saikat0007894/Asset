@{
    ViewData["Title"] = "SystemHistory";
}
@model List<OrderModel>
<h1 class="text-center">SYSTEM HISTORY</h1>
    
<table class="table table-hover">
        <tr>
            <th> OrderId </th>
            <th> EmployeeId </th>
            <th>Employee Name</th>
            <th> IssueDate </th>
            <th> SubmissionDate </th>
            <th>Fine</th>
        </tr>
        <tbody>

            @foreach (var order in Model)
            {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.EmployeeId</td>
                <td>@order.EmpName</td>
                <td>@order.IssueDate</td>
                <td>@order.SubmissionDate</td>
                <td>@order.Fine</td>
            </tr>
            
            }

        </tbody>
</table>
